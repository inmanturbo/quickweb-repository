{
    "name": "inmanturbo/quickweb-repository",
    "description": "a quick repo that implements a couple database helper interfaces",
    "license": "MIT",
    "authors": [
        {
            "name": "inmanturbo",
            "email": "inman.turbo@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Quickweb\\Repositories\\": "src"
        }
    },
    
     "autoload-dev": {
        "psr-4": {
            "Quickweb\\Tests\\": "tests/"
        }
    },
    
    "extra": {
        "laravel": {
            "providers": [
                "Quickweb\\Repositories\\RepositoriesServiceProvider"
            ]
        }
    },

    "require": {
        "illuminate/support": "^6.0",
        "illuminate/database" "^6.0"
    }
}
